&gmac1 {
    phy-mode = "rgmii";
    clock_in_out = "input";

    snps,reset-gpio = <&gpio4 RK_PC0 GPIO_ACTIVE_LOW>;
    snps,reset-active-low;
    /* Reset time is 100ms, 100ms */
    snps,reset-delays-us = <0 100000 100000>;

    assigned-clocks = <&cru SCLK_GMAC1_RX_TX>, <&cru SCLK_GMAC1>;
    assigned-clock-parents = <&cru SCLK_GMAC1_RGMII_SPEED>, <&gmac1_clkin>;
    assigned-clock-rates = <0>, <125000000>;

    pinctrl-names = "default";
    pinctrl-0 = <&gmac1m1_miim
                    &gmac1m1_tx_bus2
                    &gmac1m1_rx_bus2
                    &gmac1m1_rgmii_clk
                    &gmac1m1_rgmii_bus
                    &gmac1m1_clkinout>;

    tx_delay = <0x16>;
    rx_delay = <0x0a>;

    phy-handle = <&rgmii_phy1>;
    status = "disabled";
};

&mdio1 {
    rgmii_phy1: phy@0 {
        compatible = "ethernet-phy-ieee802.3-c22";
        reg = <0x0>;
    };
};

&combphy2_psq {
        status = "okay";
};

&i2c1 {
    status = "okay";
    clock-frequency = <100000>;
    goodix_ts@5d {
        status = "okay";
        compatible = "goodix,gt911";
        reg = <0x5d>;
        interrupt-parent = <&gpio3>;
        interrupts = <RK_PA1 IRQ_TYPE_LEVEL_LOW>;
        reset-gpios = <&gpio3 RK_PA2 GPIO_ACTIVE_LOW>;
        irq-gpios = <&gpio3 RK_PA1 GPIO_ACTIVE_HIGH>;
    };

    ov5648: camera@36 {
        compatible = "ovti,ov5648";
        reg = <0x36>;
        clocks = <&ext_cam_clk>;
        clock-names = "xvclk";
        dovdd-supply= <&cam_dovdd>; /* 1.8v */
        avdd-supply = <&cam_avdd>;  /* 2.8v */
        dvdd-supply = <&cam_dvdd>;  /* 1.5v */
        pwdn-gpios = <&gpio0 RK_PC3 GPIO_ACTIVE_HIGH>;
        reset-gpios = <&gpio0 RK_PC4 GPIO_ACTIVE_LOW>;
        rotation = <180>;
        status = "okay";

        rockchip,camera-module-index = <0>;
        rockchip,camera-module-facing = "back";
        rockchip,camera-module-name = "THDS11073";
        rockchip,camera-module-lens-name = "Largan-40122a1";

        port {
                /* MIPI CSI-2 bus endpoint */
            cam0_ov5648_out: endpoint {
                remote-endpoint = <&dphy1_in>;
                clock-lanes = <0>;
                data-lanes = <1 2>;
            };
        };
    };
};

&csi2_dphy_hw {
    status = "okay";
};

&csi2_dphy1 {
    status = "okay";
    /*
    * dphy1 only used for split mode,
    * can be used concurrently with dphy2
    * full mode and split mode are mutually exclusive
    */
    ports {
        #address-cells = <1>;
        #size-cells = <0>;

        port@0 {
            reg = <0>;
            #address-cells = <1>;
            #size-cells = <0>;

            dphy1_in: endpoint@1 {
                reg = <1>;
                remote-endpoint = <&cam0_ov5648_out>;
                data-lanes = <1 2>;
            };
        };

        port@1 {
            reg = <1>;
            #address-cells = <1>;
            #size-cells = <0>;

            dphy1_out: endpoint@1 {
                reg = <1>;
                remote-endpoint = <&isp0_in>;
            };
        };
    };
};

&rkisp {
    status = "okay";
};

&rkisp_mmu {
    status = "okay";
};

&rkisp_vir0 {
    status = "okay";
    ports {
        #address-cells = <1>;
        #size-cells = <0>;

        port@0 {
            reg = <0>;
            #address-cells = <1>;
            #size-cells = <0>;

            isp0_in: endpoint@0 {
                reg = <0>;
                remote-endpoint = <&dphy1_out>;
            };
        };
    };
};

&route_hdmi {
    status = "okay";
    connect = <&vp0_out_hdmi>;
};

&hdmi_in_vp1 {
    status = "okay";
};

&hdmi_in_vp0 {
    status = "disabled";
};

&hdmi {
    status = "okay";
};


